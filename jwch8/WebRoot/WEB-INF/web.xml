<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	
    <servlet>
        <servlet-name>aaa</servlet-name>
        <servlet-class>bit.jw.Helloworld</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>aaa</servlet-name>
        <url-pattern>/servlet/Helloworld</url-pattern>
    </servlet-mapping>
    
    <servlet>   
        <servlet-name>bbb</servlet-name>
        <servlet-class>bit.jw.Count</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>bbb</servlet-name>
        <url-pattern>/servlet/count</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>ccc</servlet-name>
        <servlet-class>bit.jw.ViewMessageServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>ccc</servlet-name>
        <url-pattern>/servlet/view</url-pattern>
    </servlet-mapping>
    	
    <servlet>
        <servlet-name>ddd</servlet-name>
        <servlet-class>bit.jw.ServletReq</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>ddd</servlet-name>
        <url-pattern>/servlet/ServletReq</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>eee</servlet-name>
        <servlet-class>bit.jw.HttpServletReq</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>eee</servlet-name>
        <url-pattern>/servlet/HttpServletReq</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>fff</servlet-name>
        <servlet-class>bit.jw.Day</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>fff</servlet-name>
        <url-pattern>/servlet/day</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ggg</servlet-name>
        <servlet-class>bit.jw.SessionLifeCycleServlet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>ggg</servlet-name>
        <url-pattern>/servlet/SessionLifeCycleServlet</url-pattern>
    </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
